(in-package :colitrsynth.audio)

(defun midino-to-freq (midino)
  (declare (optimize (speed 3) (safety 0))
           (fixnum midino))
  (* 440.0d0
     (expt 2.0d0
           (/ (the fixnum (- midino 69))
              12.0d0))))

(define-compiler-macro midino-to-freq (&whole form midino)
  (if (constantp midino)
      (funcall 'midino-to-freq midino)
      form))

